<h2>All Events</h2>

<% @events.each do |event| %>
<ul>
  <li><%= event.description %></li>
  <li><%= event.event_date %></li>
  <li><%= event.location %></li>
  <li><%= link_to 'View Event', event %></li>
</ul>
<% end %>

<%= link_to 'Create New Event', new_event_path %>
